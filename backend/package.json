		"dev": "nodemon --watch \"./src/**\" ./src/index.ts",
		"lint" : " npx eslint --ext .ts,.tsx ./src",
		"build": "tsc",
		"generate": "npx prisma generate  --schema ./src/prisma/schema.prisma",
